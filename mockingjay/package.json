{
  "name": "mockingjay",
  "version": "0.0.1",
  "description": "A mock server for http.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "forever-start": "forever start -w --watchDirectory src server.js",
    "restart": "forever restart -w --watchDirectory src server.js",
    "forever-stop": "forever stop server.js",
    "nodemon": "nodemon server.js",
    "test": "mocha --recursive"
  },
  "keywords": [
    "http",
    "response",
    "mock",
    "request",
    "verification",
    "service",
    "interface",
    "testing"
  ],
  "author": "SamuelSu",
  "license": "MIT",
  "dependencies": {
    "ajv": "^5.5.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bluebird": "^3.5.1",
    "bunyan": "^1.8.12",
    "co-body": "^5.1.1",
    "copy-to": "^2.0.1",
    "forever": "^0.15.3",
    "glob": "^7.1.2",
    "http-errors": "^1.6.2",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.3.0",
    "koa-send": "^4.1.2",
    "koa-static": "^4.0.2",
    "query-string": "^5.1.0",
    "redis": "^2.8.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "resolve-path": "^1.3.3",
    "xml": "^1.0.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "mocha": "^5.0.1",
    "mz": "^2.7.0",
    "nodemon": "^1.14.12"
  }
}
