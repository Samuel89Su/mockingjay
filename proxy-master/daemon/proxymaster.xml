<service>
	<id>proxymaster.exe</id>
	<name>ProxyMaster</name>
	<description>Proxy server for front/back-end development separately..</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>C:\Users\Administrator\AppData\Roaming\npm\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>E:\tryitout\mockingjay\proxy-master\index.js</argument>
	<argument>--log</argument>
	<argument>ProxyMaster wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<env name="NODE_ENV" value="production"/>
	<workingdirectory>E:\tryitout\mockingjay\proxy-master</workingdirectory>
</service>