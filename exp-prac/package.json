{
  "name": "exp-prac",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "forever start -w --watchDirectory ./ ./bin/www --env=production",
    "restart": "forever restart -w --watchDirectory ./ ./bin/www --env=production",
    "stop": "forever stop ./bin/www",
    "node-dev": "node ./bin/www --env=development",
    "nodemon": "nodemon ./bin/www --env=development",
    "nodemon-dist": "nodemon ./bin/www --env=production"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "connect-redis": "^3.3.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "forever": "^0.15.3",
    "http-errors": "~1.6.2",
    "http-proxy-middleware": "^0.18.0",
    "minimist": "^1.2.0",
    "morgan": "~1.9.0",
    "pug": "^2.0.3",
    "query-string": "^6.0.0",
    "rotating-file-stream": "^1.3.5",
    "ws": "^5.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.17.2"
  }
}
